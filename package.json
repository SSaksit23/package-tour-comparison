    "html2canvas": "^1.4.1",
    "idb": "^8.0.0",
    "jspdf": "^2.5.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.454.0",
    "mammoth": "^1.8.0",
    "maplibre-gl": "^5.15.0",
    "next-themes": "^0.4.6",
    "pdfjs-dist": "4.5.136",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^2.5.4",
