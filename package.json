{
  "name": "itinerary-analyzer-program",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:docker": "docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "test:docker:stop": "docker-compose -f docker-compose.test.yml down",
    "docker:dev": "docker-compose up dev --build",
    "docker:prod": "docker-compose up prod --build",
    "docker:stop": "docker-compose down",
    "start": "powershell -ExecutionPolicy Bypass -File start-dev.ps1"
  },
